package main 

import (
	"os"
	"fmt"
)

func main() {
	for i, arg := range os.Args[0:] {
		fmt.Println(i,arg);
	}	
}
package main

import (
	"fmt"
	"os"
	"strings"
)

func main() {
	fmt.Println(strings.Join( os.Args[0:], " "))	
}

